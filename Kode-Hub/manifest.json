{
    "manifest_version": 2,
    "name": "KodeHub",
    "version": "1.0",
  
    "description": "This extension adds a browser action icon to the toolbar. Click the button to choose a course link. ",
    "icons": {
      "48": "icons/book.png"
    },
  
    "permissions": [
      "activeTab"
    ],
  
    "browser_action": {
      "default_icon": "icons/book.png",
      "default_title": "KodeHub",
      "default_popup": "popup/choose_hub.html"
    }
  }
  
  